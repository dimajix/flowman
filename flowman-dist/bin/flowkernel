#!/usr/bin/env bash

basedir=$(readlink -f $(dirname $0)/..)
source $basedir/libexec/flowman-common.sh

APP_NAME="flowman-studio-kernel"
APP_VERSION="${project.version}"
APP_MAIN="com.dimajix.flowman.studio.kernel.Application"

APP_JAR=$FLOWMAN_HOME/lib/"$APP_NAME-$APP_VERSION.jar"
LIB_JARS="${flowman-studio-kernel.classpath}"

spark_submit $APP_JAR $LIB_JARS $APP_MAIN "$@"
